    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-104952515-1', 'auto');
        ga('send', 'pageview');

        $(document).ready(function () {
            $('#layoutsLink').on('click', function (e) {
                e.preventDefault();
                var link = $(this).attr('href');
                window.location.href = link;
            });
            // Gắn sự kiện click cho thẻ a có ID là "mailboxLink"
            $('#mailboxLink').on('click', function (e) {
                // Ngăn chặn hành vi mặc định của thẻ a (chuyển hướng trang)
                e.preventDefault();

                // Lấy đường link từ thuộc tính href
                var link = $(this).attr('href');

                // Chuyển hướng đến đường link
                window.location.href = link;
            });
            $('#calendarLink').on('click', function (e) {
                e.preventDefault();
                var link = $(this).attr('href');
                window.location.href = link;
            });
            $('#accountLink').on('click', function (e) {
                e.preventDefault();
                var link = $(this).attr('href');
                window.location.href = link;
            });
        });
    </script>
