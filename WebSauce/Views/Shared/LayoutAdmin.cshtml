                                </li>
                                <li class="mobile-menu-toggle">
                                    <a href="#" class="menu-toggle wave-effect">
                                        <i class="feather icon-menu"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>


            <!-- Main Menu -->
            <div class="sidebar">
                <div class="logo">
                    <a href="/">
                        <span class="logo-emblem"><img src="~/Images/logox2.png" alt="BA" /></span>
                        <span class="logo-full">admin</span>
                    </a>
                </div>
                <ul id="sidebarCookie">
                    <li class="spacer"></li>
                    <li class="profile">
                        <span class="profile-image">
                            <img src="~/Images/logoadmin-removebg-preview.png" alt="profile" />
                        </span>
                        <span class="profile-name">
                            Lê Hoài Nam
                        </span>
                        <span class="profile-info">
                            Premium Admin
                        </span>
                    </li>
                    <li class="spacer"></li>
                    <li class="title">
                        <i class="feather icon-more-horizontal"></i>
                        <span class="menu-title">Trang chủ</span>
                    </li>
                    <li class="nav-item">
                        <a id="layoutsLink" class="nav-link wave-effect collapsed" href="@Url.Action("Index","Home")" data-parent="#sidebarCookie" data-toggle="collapse" aria-expanded="false" aria-controls="page-layouts">
                            <i class="feather icon-sidebar"></i>
                            <span class="menu-title">Page Layouts</span>
                            <i class="feather icon-chevron-down down-arrow"></i>
                        </a>
                    </li>

                    <li class="title">
                        <i class="feather icon-more-horizontal"></i>
                        <span class="menu-title">Chức năng</span>
                    </li>
                    <li class="nav-item">
                        <a id="mailboxLink" href="@Url.Action("laydsSP","Admin")" class="nav-link wave-effect collapsed" data-parent="#sidebarCookie" data-toggle="collapse" aria-expanded="false" aria-controls="page-mailbox">
